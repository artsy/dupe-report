{
  "name": "@artsy/dupe-report",
  "description": "A tool for reporting on duplicate dependencies in a webpack bundle",
  "version": "0.0.1",
  "author": "Justin Bennett <zephraph@gmail.com>",
  "files": [
    "index.js",
    "/lib"
  ],
  "homepage": "https://github.com/artsy/dupe-report",
  "license": "MIT",
  "main": "lib/index.js",
  "repository": "https://github.com/artsy/dupe-report.git",
  "scripts": {
    "prepack": "yarn now-build",
    "now-build": "rm -rf lib && tsc -b",
    "lint": "tslint -p . -t stylish",
    "type-check": "tsc --pretty --noEmit"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "bugs": "https://github.com/artsy/dupe-report/issues",
  "dependencies": {
    "axios": "0.18.0",
    "@octokit/plugin-retry": "2.2.0",
    "@octokit/plugin-throttling": "2.4.0",
    "@octokit/rest": "16.16.0",
    "@slack/client": "4.9.0",
    "circleci-api": "3.3.1",
    "diff": "4.0.1",
    "dotenv": "6.2.0",
    "lodash.zip": "4.2.0"
  },
  "devDependencies": {
    "@artsy/auto-config": "0.1.0",
    "@types/dotenv": "6.1.0",
    "@types/lodash.zip": "4.2.5",
    "@types/node": "10.12.27",
    "@types/diff": "4.0.1",
    "ts-node": "8.0.2",
    "tslint": "5.13.0",
    "typescript": "3.3.3333"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "types": "lib/index.d.ts"
}
